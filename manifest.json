{
  "manifest_version": 2,
  "name": "Software Security",
  "version": "0.1",
  "permissions": [
      "https://naver.com/*"
  ],
  "externally_connectable": {
    "matches": ["*://*.naver.com/*"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ]
}
